
public class StringReverse {

	public static void main(String[] args) {
		
		String s = "nanna";
		
		String res = strRev(s);
		
		System.out.println(res);
		
	}

	private static String strRev(String s) {
		
		int len= s.length();
		
		
		for(int i=0;i<len/2;i++){
			
			
		}
		
		
		return s;
		
	}
	
}
