	public static void main(String[] args) {
		
		int a[] = {1,60,-10,70,-80,85};
		
		Arrays.sort(a);
		
		// final result
		int fdiff = a[0] - a[1];
		
		//Distance from 0
		int inv = Math.abs(0-fdiff);
		
		int min1 = a[0],min2 = a[1];
		
		
		
	}
	
}
